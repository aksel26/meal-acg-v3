    "@types/react": "^19.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.31.0",
    "lucide": "^0.534.0",
    "lucide-react": "^0.533.0",
    "next-themes": "^0.4.6",
